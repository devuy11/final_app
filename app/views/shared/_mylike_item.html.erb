<li id="<%= mylike_item.id %>">
    <span class="content"><%= mylike_item.content %></span>

    <%= render partial: 'shared/post_common', locals: { post: mylike_item, is_profile: "1" } %>

  <% if current_user==(mylike_item.user) %>
    <%= link_to "delete", mylike_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: mylike_item.content %>
  <% end %>
</li>